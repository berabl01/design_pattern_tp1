package pattern_singleton;

public class Aeroport {
	public Aeroport() {
		boolean pist_libre = true;
	}

}

 class Avion  extends Thread{
	String nom;
	Aeroport a;
	public Avion( String s) {
		nom=s;
	}
	
	public void run () {
		a=new Aeroport();
		System.out.println("Je_suis_l'avion"+nom+"_sur_1'aeroport"+a);
	}

}

class testaeroport {
	public static void main(String [] args) {
		Avion v1 = new Avion("Avion_1");
		Avion v2 = new Avion("Avion_2");
		Avion v3 = new Avion("Avion_3");
		Avion v4 = new Avion("Avion_4");
		v1.start();
		v2.start();
		v3.start();
		v4.start();
	}
}
